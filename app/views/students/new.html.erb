<form action="students" method="POST">
    <label>Student Form</label>
    <input type="text" id="student[first_name]" name='student[first_name]'>
    <input type="text" id="student[last_name]" name='student[last_name]'>
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="submit" value="Submit Student">

</form>

